	"dependencies": {
		"autoprefixer": "^9.6.1",
		"node-sass": "^4.12.0",
		"postcss-cli": "^6.1.2"
	},
	"scripts": {
		"watch:css": "node-sass -w src/App.scss docs/main.min.css --output-style compressed",
		"watch:prefix": "postcss docs/main.min.css -o docs/main.min.css --watch --use autoprefixer",
		"watch:html": "browser-sync start --server docs/ --no-open",
		"dev": "npm-run-all --parallel watch:**"
	},
	"devDependencies": {
		"browser-sync": "^2.26.7",
		"npm-run-all": "^4.1.5"
	},
